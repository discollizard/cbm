@model IEnumerable<TesteCobmais.Models.LogConsulta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
   
    <a asp-action="ProcessarArquivoCSV">Processar arquivo disponível no servidor</a>
    @if (!ViewData.ModelState.IsValid)
    {
        <div class="alert alert-danger" style="margin-top: 20px;">
            @Html.ValidationSummary()
        </div>
    }
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ConsultaTimestamp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AtrasoEmDias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAtualizado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescontoMaximo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.contrato)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ConsultaTimestamp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AtrasoEmDias)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorAtualizado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescontoMaximo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contrato.DividaId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
